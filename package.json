{
  "name": "abc-todolist",
  "version": "1.0.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abc-software-solutions-company/abc-website.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-dev": "docker-compose up && yarn dev",
    "build-stats": "cross-env ANALYZE=true yarn build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "format": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-all": ".husky/pre-commit",
    "clean": "rimraf \"{.next,dist,build,out,node_modules,coverage,*.tsbuildinfo}\"",
    "postbuild": "next-sitemap",
    "cm": "git-cz",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --watchAll=false --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --silent --watchAll=false --coverage && yarn coverage:open",
    "coverage:open": "open-cli coverage/lcov-report/index.html -- 'google chrome'",
    "dev:lint": "npm-run-all --print-label --parallel dev lint",
    "genfont": "rimraf src/vendors/abc-icons/dist && node ./cli/generate-font.js",
    "db": "json-server --watch src/api/fixture/db.json --port 3100"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@headlessui/react": "^1.6.4",
    "@hookform/resolvers": "^2.9.7",
    "@mui/material": "^5.10.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "is-url-http": "^2.3.2",
    "jsonp": "^0.2.1",
    "lodash-es": "^4.17.21",
    "next": "12.2.0",
    "next-i18next": "^11.0.0",
    "next-seo": "^5.4.0",
    "next-share": "^0.17.0",
    "nextjs-progressbar": "^0.0.14",
    "qs": "^6.11.0",
    "react": "18.1.0",
    "react-content-loader": "^6.2.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.34.2",
    "react-markdown": "^8.0.3",
    "react-query": "^3.39.1",
    "react-transition-group": "^4.4.5",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.30.7",
    "socket.io-client": "^2.3.1",
    "swiper": "^8.3.2",
    "tiny-slider": "^2.9.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "^28.1.1",
    "@types/jsonp": "^0.2.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "17.0.36",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-transition-group": "^4.4.5",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.11",
    "enforce-branch-name": "^1.1.2",
    "eslint": "8.16.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.5.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module-scoped": "^1.0.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-junit": "^13.2.0",
    "json-server": "^0.17.0",
    "next-compose-plugins": "^2.2.1",
    "next-sitemap": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "svgtofont": "^3.17.8",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "engines": {
    "node": ">=14.18"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module-scoped",
      "parser": "typescript"
    }
  }
}
